{
  "name": "eslint-config-norton",
  "version": "1.2.2",
  "description": "ESLint shareable config for the Norton JavaScript style guide",
  "keywords": [
    "code",
    "eslint",
    "lint",
    "linter",
    "quality",
    "style"
  ],
  "homepage": "https://gitlab.com/wwnorton/style/eslint-config-norton",
  "bugs": {
    "url": "https://gitlab.com/wwnorton/style/eslint-config-norton/issues"
  },
  "license": "MIT",
  "author": "Evan Yamanishi",
  "files": [
    ".eslintignore",
    ".eslintrc.js",
    "index.js",
    "rules",
    "test"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:wwnorton/style/eslint-config-norton.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags",
    "report": "nyc report --reporter=html",
    "test": "nyc ava"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^12.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "ava": "^0.24.0",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.7.0",
    "nyc": "^11.4.0"
  },
  "peerDependencies": {
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.7.0"
  },
  "ava": {
    "files": [
      "test"
    ],
    "failWithoutAssertions": false,
    "serial": true,
    "verbose": true
  }
}
