{
  "name": "eslint-config-norton",
  "version": "2.0.1",
  "description": "ESLint shareable config for the Norton JavaScript style guide",
  "keywords": [
    "code",
    "eslint",
    "lint",
    "linter",
    "quality",
    "style"
  ],
  "homepage": "https://gitlab.com/wwnorton/style/eslint-config-norton",
  "bugs": {
    "url": "https://gitlab.com/wwnorton/style/eslint-config-norton/issues"
  },
  "license": "MIT",
  "author": "Evan Yamanishi",
  "files": [
    ".eslintignore",
    ".eslintrc.js",
    "index.js",
    "rules",
    "test"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:wwnorton/style/eslint-config-norton.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "release": "standard-version",
    "test": "ava"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^13.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "ava": "^1.0.1",
    "eslint": "^5.12.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "standard-version": "^4.4.0"
  },
  "peerDependencies": {
    "eslint": "^4.19.1 || ^5.3.0",
    "eslint-plugin-import": "^2.14.0"
  },
  "ava": {
    "files": [
      "test"
    ],
    "failWithoutAssertions": false,
    "serial": true,
    "verbose": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  }
}
